---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ site.url }}</loc>
    </url>

    {% for post in site.posts %}
    <url>
        <loc>{{ site.url }}{{ post.url }}</loc>
    </url>
    {% endfor %}

    {% for demo in site.categories.demos %}
    <url>
        <loc>{{ site.url }}{{ demo.url }}</loc>
    </url>
    {% endfor %}
</urlset>
